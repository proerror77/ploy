# MiniMax M2.5 via Anthropic-compatible API
#
# This keeps the Claude Agent SDK code unchanged, but routes requests to MiniMax.
# Replace placeholders with your actual keys/URLs.

# MiniMax key (Anthropic-compatible)
ANTHROPIC_API_KEY=sk-...
ANTHROPIC_BASE_URL=https://api.minimax.io/anthropic

# Ploy Rust Backend
PLOY_API_URL=http://localhost:8081
PLOY_API_KEY=

# Grok (X.com search)
GROK_API_KEY=

# Sidecar config
SIDECAR_MODEL=MiniMax-M2.5
SIDECAR_POLL_INTERVAL_SECS=300
SIDECAR_MAX_BUDGET_USD=1.00
SIDECAR_DRY_RUN=true

